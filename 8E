#include <bits/stdc++.h>

using namespace std;

int main()
{
    double a,b,c,x1,x2,y1,y2;
    cin >> x1 >> y1 >> x2 >> y2 >> a >> b >>c;
    if(a*x1+b*y1+c < 0){
        if(a*x2+b*y2+c < 0){
            cout << "YES";
            return 0;
        }
    }
    if(a*x1+b*y1+c > 0){
        if(a*x2+b*y2+c > 0){
            cout << "YES";
            return 0;
        }
    }
    cout << "NO";
}
