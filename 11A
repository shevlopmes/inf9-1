#include <bits/stdc++.h>

using namespace std;

int main()
{
    double t[3][2];
    for(int i = 0; i < 3; ++i){
        for(int j = 0; j < 2; ++j){
            cin >> t[i][j];
        }
    }
    cout.precision(20);
    cout << (t[0][0]+t[1][0]+t[2][0])*1./3 << " " << (t[0][1]+t[1][1]+t[2][1])*1./3;
}
